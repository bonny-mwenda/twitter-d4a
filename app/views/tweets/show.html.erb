<h1>User Tweets</h1>
<table class="table table-striped table-hover">
	<thead>
		<tr>
			<th>User</th>
			<th>Status</th>
			<th>Date Created</th>
		</tr>
	</thead>
	<tbody>
	<% @tweets.each do |tweet| %>
		<tr>
			<th><%= tweet.user_id %></th>
			<th><%= tweet.status %></th>
			<th><%= tweet.created_at %></th>
		</tr>
		<% end %>
	</tbody>
</table>
