<h1>MyTwitter App Users</h1>

<table class="table table-striped table-hover">
	<thead>
		<tr>
			<th>User</th>
			<th>Gender</th>
			<th>Email</th>
			<th>Profile Picture</th>
			<th>Date Created</th>
		</tr>
	</thead>
	<tbody>
	<% @users.each do |user| %>
		<tr>
			<th><%= user.username %></th>
			<th><%= user.gender %></th>
			<th><%= user.email %></th>
			<th><%= user.profile_picture %></th>
			<th><%= user.created_at %></th>
		</tr>
		<% end %>
	</tbody>
</table>